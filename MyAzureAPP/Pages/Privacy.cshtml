@page
@using MyAzureAPP.Model
@model PrivacyModel


<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<table>
    <thead>
    <th>Id</th>
    <th>Name</th>
    <th>Department</th>
    <th>Salary</th>
    </thead>
    <tbody>
        @foreach (var item in Model.employeesList)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Department</td>
                <td>@item.Salary</td>
            </tr>

        }
    </tbody>
</table>

